      this.puntosTxt = new PIXI.Text('puntos', ct.MainFontSM);

      this.puntosTxt.x = ct.camera.width-90

      this.puntosTxt.y = 60;


      this.addChild(this.puntosTxt)



      // COUNTER

      this.countText = '1:00';

      this.countdownTxt = new PIXI.Text(this.countText, ct.MainFontBoldMD);

      this.countdownTxt.x = 20

      this.countdownTxt.y = 8;

      this.addChild(this.countdownTxt)


      // COUNTER BAR

      var counterBar = new PIXI.Graphics();

      counterBar.beginFill(0x492fe5);

      counterBar.drawRoundedRect(20, 60, 90 , 18, 6);

      counterBar.endFill();

      this.addChild(counterBar);


      // COUNTER BAR TIME

      this.counterBarTime = new PIXI.Graphics();

      this.counterBarTime.beginFill(0xffffff);

      //counterBarTime.drawRoundedRect(24, 64, 82-(82/(60/this.countdown)), 10,
      4);

      this.counterBarTime.drawRoundedRect(0, 64, 82, 10, 4);

      this.counterBarTime.endFill();

      this.counterBarTime.x = 24;

      this.addChild(this.counterBarTime);



      // TIMER

      this.countdown = 60

      var timerinterval = setInterval(()=>{
          this.countdown--;
          
          // Label count
          var counttag = '0:'+this.countdown
          if(this.countdown<10){ counttag = '0:0'+this.countdown }
          this.countText = counttag
          
          // End Timer
          if(this.countdown==0){
              endGame()
          }
      }, this.timerSpeed)



      // CREATE


      var creatorInterval = setInterval(()=>{
          var coin = ct.templates.copy("coin", (Math.random() * (ct.camera.width-100))+50, 100)
          coin.speed = 4
          coin.direction = 90

          if(coin.y>ct.camera.height){
              coin.kill = true;
          }
      }, 500);



      // SET SCORE

      this.setScore  = function (item) {
          ct.sound.spawn('ok');
          //Add points
          var points = 0
          var coin = item.type
          if(coin==0){ points = 100 }
          if(coin==1){ points += 200 }
          if(coin == 2){ if(ct.score>300){ points = -300 } else { points = ct.score } }

          ct.score += points
          var pointview = ct.templates.copy("points", item.x, item.y)
          pointview.pointLabel.text = points;
      }





      /* END GAME */

      function endGame () {
          clearInterval(timerinterval)
          clearInterval(creatorInterval)
          ct.pixiApp.ticker.speed = 0;
          setTimeout(()=>{
              ct.rooms.switch('04End')
          }, 250)
      }


    onstep: |+
      this.counterBarTime.width = 82-(82/(60/this.countdown))


