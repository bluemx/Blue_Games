  - type: template
    name: alcancia
    depth: 0
    oncreate: |+
      var _this = this
      var _this = this
      this.dir = 'c'
      this.dragging = false;
      this.speedmove = 8;

    onstep: |
      var _this = this
      this.move();

      //Animation Sprite Direction
      if(this.dir=='l'){
          this.gotoAndStop(2);
      }
      if(this.dir=='c'){
          this.gotoAndStop(1);
      }
      if(this.dir=='r'){
          this.gotoAndStop(0);
      }



      /* DRAGGING */

      if (ct.touch.hovers(this) && ct.actions.TouchAction.pressed) {
          this.dragging = true;
      }

      if (this.dragging && ct.actions.TouchAction.released) {
          this.dragging = false;
          this.dir = 'c'
      }


      // DRAG MOVEMENT
      if (this.dragging) {
          var mouseX = Math.round(ct.touch.x)
          var itemX = Math.round(this.x)

          if(Math.abs(mouseX - itemX) < 10){
              this.dir = 'c'
              return false
          }
          if(mouseX>itemX ){
              moveItem('r')
          } else if (mouseX){
              moveItem('l')
          }
      }


      // KEYBOARD MOVEMENTE
      if(ct.actions.MoveAction.down){
          if(ct.actions.MoveAction.value>0){
              moveItem('r')
          } else if(ct.actions.MoveAction.value<0) {
              moveItem('l')
          }
      }
      if(ct.actions.MoveAction.released){
          this.dir = 'c'
      }


      function moveItem (dir) {
          _this.dir = dir
          if(dir=='r' && (_this.x < ct.camera.width-_this.width/2)){
              _this.x += _this.speedmove
          } else if (dir=='l' && _this.x > _this.width/2){
              _this.x -= _this.speedmove
          }
      }
    ondraw: ''
    ondestroy: ''
