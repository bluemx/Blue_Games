  - type: template
    name: coinNoFN
    depth: 0
    oncreate: |+

      this.constant = 25 * Math.random()
      this.angle = 0.005
      this.scalar = Math.round(Math.random()*20)
      this.speed = (Math.random()*0.1) - 0.05

      if(this.fix){
          this.speed = 0
          this.width = 50
          this.height = 50
      }

      this.pos = {
          x: this.x,
          y: this.y
      }


      if(this.type == undefined){
          this.type = Math.round(Math.random()*2)
      }
      this.gotoAndStop(this.type)

    onstep: |-
      this.move();


      /* circular motion */
      if(!this.fix){
          this.movX = this.constant + Math.sin(this.angle) * this.scalar;
          this.movY = this.constant + Math.cos(this.angle) * this.scalar;
          this.x = this.pos.x + this.movX
